package com.sviatlana.shapeCalculation.exception;

public class ZeroNotAllowed extends Exception {

	public ZeroNotAllowed() {}  
	
	public ZeroNotAllowed(String er) { 
		super(er); 
	}  
	
	public ZeroNotAllowed (Throwable er) {
        super (er);
    }

    public ZeroNotAllowed (String er, Throwable cause) {
        super (er, cause);
    }

}
