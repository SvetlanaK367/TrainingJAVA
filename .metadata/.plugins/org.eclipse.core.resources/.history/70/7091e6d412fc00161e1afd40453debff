package com.sviatlana.library.model;

import java.util.Comparator;

public class ItemCompare implements Comparator<Publication>, Comparable<Publication> {

	// Overriding the compareTo method
	public int compareTo(Publication p) {
	    return (this.name).compareTo(p.name);
	}

    @Override
    public int compare(Publication p1, Publication p2) {
        if (a.toString().compareToIgnoreCase(b.toString())>0)
            return 1;
        else if (a.toString().compareToIgnoreCase(b.toString())<0)
            return -1;
        return 0;
    }
}
